   "source": [
    "text = \"hello world\" \n",
    "brokenLetters = \"ad\"\n",
    "\n",
    "broken_letter_set = set(brokenLetters)\n",
    "words = text.split()\n",
    "count = 0\n",
    "\n",
    "for word in words:\n",
    "    if all(ch not in broken_letter_set for ch in word):\n",
    "        count += 1\n",
    "\n",
    "count"
   ]
  }
