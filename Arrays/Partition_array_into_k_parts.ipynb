    "# Partition Array into K distinct parts"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "dec09bb5",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "True\n"
     ]
    }
   ],
   "source": [
    "from collections import Counter\n",
    "\n",
    "nums = [1,2,3,4]\n",
    "k = 2\n",
    "\n",
    "if len(nums)%k != 0:\n",
    "    print(False)\n",
    "\n",
    "freq = Counter(nums)\n",
    "groups = len(nums) // k\n",
    "\n",
    "for count in freq.values():\n",
    "    if count>groups:\n",
    "        print(False)\n",
    "\n",
    "print(True)"
   ]
  }
