     "text": [
      "Minimum path:  -1\n"
     ]
    }
   ],
   "source": [
    "triangle = [[-1],[2,3],[1,-1,-3]]\n",
    "\n",
    "dp = triangle[-1][:]\n",
    "\n",
    "for row in range(len(triangle)-2, -1, -1):\n",
    "    for i in range(len(triangle[row])):\n",
    "        dp[i] = triangle[row][i] + min(dp[i], dp[i+1])\n",
    "\n",
    "print(\"Minimum path: \", dp[0])"
   ]
  }
