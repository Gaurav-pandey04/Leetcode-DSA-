   "source": [
    "n=6\n",
    "\n",
    "if n==0 or n==1:\n",
    "    print(n)\n",
    "dp = [0]*(n+1)\n",
    "dp[0], dp[1], dp[2] = 0, 1, 1\n",
    "\n",
    "for i in range(3, n+1):\n",
    "    dp[i] = dp[i-1] + dp[i-2] + dp[i-3]\n",
    "\n",
    "dp[n]"
